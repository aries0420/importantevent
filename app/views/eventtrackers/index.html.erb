<h1>Listing eventtrackers</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @eventtrackers.each do |eventtracker| %>
  <tr>
    <td><%= eventtracker.title %></td>
    <td><%= eventtracker.description %></td>
    <td><%= eventtracker.date %></td>
    <td><%= link_to 'Show', eventtracker %></td>
    <td><%= link_to 'Edit', edit_eventtracker_path(eventtracker) %></td>
    <td><%= link_to 'Destroy', eventtracker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Eventtracker', new_eventtracker_path %>
